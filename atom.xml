---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

<title>CSIRT Foundry</title>
<link href="http://csirtfoundry.com/atom.xml" rel="self"/>
<link href="http://csirtfoundry.com/"/>
<updated>{{ site.time | date_to_xmlschema }}</updated>
<id>http://www.csirtfoundry.com/</id>
<author>
    <name>Chris Horsley</name>
    <email>contact@csirtfoundry.com</email>
</author>

{% for post in site.posts %}
<entry>
    <title>{{ post.title }}</title>
    <link href="http://csirtfoundry.com{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://csirtfoundry.com{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
</entry>
{% endfor %}

</feed>
